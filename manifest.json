{
    "manifest_version": 3,
    "name": "PhishingWarning",
    "version": "1.0",
    "description": "Detecta correos electr√≥nicos de phishing en Gmail.",
    "permissions": ["activeTab", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [{
      "matches": ["*://mail.google.com/*"],
      "js": ["content.js"]
    }],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  